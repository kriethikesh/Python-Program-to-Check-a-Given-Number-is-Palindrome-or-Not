    {
      "cell_type": "code",
      "source": [
        "x=int(input(\"enter a number\"))\n",
        "y=x\n",
        "a=0\n",
        "while x>0:\n",
        "  d=x%10\n",
        "  a=a*10+d\n",
        "  x=x//10\n",
        "if a==y:\n",
        "  print(y,\"is palindrome number\")\n",
        "else:\n",
        "  print(y,\"is not palindrome number\")"
      ],
